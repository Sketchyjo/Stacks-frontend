{
  "expo": {
    "name": "Stacks",
    "slug": "Stacks",
    "version": "1.0.0",
    "scheme": "stacks",
    "platforms": ["ios", "android"],
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/favicon.png"
    },
    "plugins": ["expo-router", "expo-splash-screen"],
    "experiments": {
      "typedRoutes": false,
      "tsconfigPaths": true
    },
    "orientation": "portrait",
    "icon": "./assets/icon/ios-light.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "buildNumber": "1.0.0",
      "bundleIdentifier": "com.stacksmobile.Stacks",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      },
        "icon": {
                "dark": "./assets/icon/ios-dark.png",
                "light": "./assets/icon/ios-light.png",
                "tinted": "./assets/icon/ios-tinted.png"
       }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.stacksmobile.Stacks"
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "9cd7e395-5696-4911-85c5-ba9b8b6788c6"
      }
    }
  }
}
